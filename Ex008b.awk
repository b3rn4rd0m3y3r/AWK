BEGIN { FS = "[., ]"; H = 0; }
{
    S0 = ($4 ~ /^[[:digit:]]/ && H == 0);
    S1 = ($5 ~ /^[[:digit:]]/ && H == 0);
    S2 = ($6 ~ /^[[:digit:]]/ && H == 0);
    S3 = ($7 ~ /^[[:digit:]]/ && H == 0);
    S = S0*1+S1*2+S2*4+S3*8
    print $4 "|" $5 "|" $6 "|" $7 "|" S
    CAMPO = (S==7)*4+(S==12)*6+(S==14)*5+(S==15 || S==8)*7
    print $CAMPO
}
END {}
